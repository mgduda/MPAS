<!-- **************************************************************************************** -->
<!-- ************************************** Dimensions ************************************** -->
<!-- **************************************************************************************** -->

        <dims>
                <dim name="nCells"/>
                <dim name="nEdges"/>
                <dim name="maxEdges"/>
                <dim name="maxEdges2"/>
                <dim name="nVertices"/>
                <dim name="TWO"                definition="2"/>
                <dim name="THREE"              definition="3"/>
                <dim name="vertexDegree"/>
